{
  "name": "@suseejs/types",
  "version": "0.1.2",
  "description": "Type definitions for suseejs",
  "type": "module",
  "main": "dist/commonjs/index.js",
  "module": "dist/index.js",
  "types": "dist/commonjs/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/commonjs/index.js",
        "default": "./dist/commonjs/index.d.ts"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "commonjs": "tsc -p tsconfig.commonjs.json",
    "esm": "tsc -p tsconfig.esm.json",
    "build": "npm run esm && npm run commonjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suseejs/types.git"
  },
  "keywords": [
    "TypeScript"
  ],
  "author": {
    "name": "Pho Thin Mg",
    "email": "phothinmg@disroot.org",
    "url": "https://phothinmg.github.io/"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/suseejs/types/issues"
  },
  "homepage": "https://github.com/suseejs/types#readme",
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
